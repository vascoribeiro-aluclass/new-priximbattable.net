<Order>
	<OrderNumber>{{reference}}</OrderNumber>
	<CustomerNumber>{{id_customer}}</CustomerNumber>
	<Currency>{{currency}}</Currency>
	<Language>{{customer_country_iso}}</Language>
	<Locale>{{customer_country_iso}}</Locale>
	<TaxArea>EU</TaxArea>
	<TaxModel>gross</TaxModel>
	<GrandTotal>{{total_paid_tax_incl}}</GrandTotal>
	<TotalBeforeTax>{{total_paid_tax_excl}}</TotalBeforeTax>
	<TotalTax>{{total_tax}}</TotalTax>
	<Addresses>
		<BillingAddress>
			<FirstName>{{customer_firstname}}</FirstName>
			<LastName>{{customer_lastname}}</LastName>
			<AddressExtension>{{customer_address}}</AddressExtension>
			<Street>{{customer_address}}</Street>
			<Street2>{{customer_address}}</Street2>
			<Zipcode>{{customer_postcode}}</Zipcode>
			<City>{{customer_city}}</City>
			<EMail>{{customer_email}}</EMail>
			<PhoneCell>{{customer_phone_mobile}}</PhoneCell>
		</BillingAddress>
		<ShippingAddress>
			<FirstName>{{icustomer_firstname}}</FirstName>
			<LastName>{{icustomer_lastname}}</LastName>
			<AddressExtension>{{icustomer_address}}</AddressExtension>
			<Street>{{icustomer_address}}</Street>
			<Street2>{{icustomer_address}}</Street2>
			<Zipcode>{{icustomer_postcode}}</Zipcode>
			<City>{{icustomer_city}}</City>
			<EMail>{{customer_email}}</EMail>
			<PhoneCell>{{icustomer_phone_mobile}}</PhoneCell>
		</ShippingAddress>
	</Addresses>
	<LineItems>
		{{product_lines_xml}}
		<LineItemShipping>
			<Id>{{carrier}}</Id>
			<TotalPrice>{{total_shipping_tax_incl}}</TotalPrice>
			<Name>{{carrier}}</Name>
			<TaxRate>{{carrier_tax_rate}}</TaxRate>
			<TaxComponent>
				<TaxRate>{{carrier_tax_rate}}</TaxRate>
				<Value>{{total_shipping_tax_incl}}</Value>
			</TaxComponent>
		</LineItemShipping>
		<LineItemPayment>
			<Id>{{module}}</Id>
			<TotalPrice>0.00</TotalPrice>
			<Name>{{module}}</Name>
			<TaxRate>{{carrier_tax_rate}}</TaxRate>
		</LineItemPayment>
	</LineItems>
	<CreationDate>{{date_add}}</CreationDate>
	<CustomerComment><![CDATA[
		{{message}}
	]]></CustomerComment>
	<PaidOn>{{date_upd}}</PaidOn>
</Order>
